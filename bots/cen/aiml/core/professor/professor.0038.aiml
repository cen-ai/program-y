<template><srai>SET PREDICATES</srai>
<srai>SET PREDICATES</srai>
<think><set name="topic">your son</set></think>
<think><set name="emotion">interested</set></think>
<condition name="son">
  <li value="OM">
    <random>
      <li>Have you forgotten your son's name?</li>
      <li>Have you forgotten?</li>
    </random>
  </li>
  <li value="unknown">
    <random>
      <li>Have you forgotten your son's name?</li>
      <li>Have you forgotten?</li>
    </random>
  </li>
  <li>Your son's name is <get name="son"/>.</li>
</condition>
</template>
</category>
